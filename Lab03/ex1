//Lab03 ex 01
#include <stdio.h>
#include <stdlib.h>

int main(){

    int int_1, int_2;
    float float_1, float_2;

    printf("Insert the first integer\n");
    scanf("%d", &int_1);
    printf("Insert the second integer\n");
    scanf("%d", &int_2);
    printf("Insert the first real number\n");
    scanf("%g", &float_1);
    printf("Insert the second real number\n");
    scanf("%g", &float_2);
    printf("%5d %5d\n%5.2g\n%.3g", int_1, int_2, float_1, float_2);

    return 0;
}
